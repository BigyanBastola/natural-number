import java.sql.SQLOutput;
import java.util.Scanner;
public class dowhileloop {
    public static void main( String args[]){
        int num;
        System.out.println("Enter a number:=");
        Scanner obj = new Scanner(System.in);
        num =obj.nextInt();
        do {
            System.out.println(num);
        ++num;
        }

        while(num<=10);

        }
    }

